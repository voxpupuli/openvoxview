{
  "name": "openvoxview-ui",
  "type": "module",
  "version": "0.0.1",
  "description": "UI for openvoxview",
  "productName": "OpenVox View UI",
  "author": "Sebastian Rakel <sebastian@devunit.eu>",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "i18n": "vue-i18n-extract report --add --vueFiles './src/**/*.?(ts|vue)' --languageFiles './src/i18n/langs/*.json' --noEmptyTranslation '*'",
    "postinstall": "quasar prepare"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "axios": "^1.2.1",
    "moment": "^2.30.1",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.3.0",
    "quasar": "^2.16.0",
    "vue": "^3.5.20",
    "vue-i18n": "^11.0.0",
    "vue-router": "^4.6.0",
    "vue3-json-viewer": "^2.4.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@intlify/unplugin-vue-i18n": "^11.0.1",
    "@quasar/app-vite": "^2.4.0",
    "@types/node": "^24.7.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^10.5.0",
    "globals": "^16.4.0",
    "prettier": "^3.3.3",
    "typescript": "~5.9.2",
    "vite-plugin-checker": "^0.11.0",
    "vue-eslint-parser": "^10.2.0",
    "vue-i18n-extract": "^2.0.7",
    "vue-tsc": "^3.1.2"
  },
  "engines": {
    "node": "^24 || ^22 || ^20",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
